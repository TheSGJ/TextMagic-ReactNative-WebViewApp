{
  "expo": {
    "name": "TextMagic",
    "slug": "TextMagic",
    "version": "1.1.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#87CEEB"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.thesgj.textmagic",
      "buildNumber": "1.1.2",
      "supportsTablet": true
    },
    "android": {
      "package": "com.thesgj.textmagic",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#87CEEB"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
